{
    "name": "oliver-hader/demo-comments",
    "type": "typo3-cms-extension",
    "description": "INSECURE TYPO3 Comments Extension (demo)",
    "authors": [
        {
            "name": "Oliver Hader",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "OliverHader\\DemoComments\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OliverHader\\DemoComments\\Tests\\": "Tests"
        }
    },
    "replace": {
        "demo_comments": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "demo_comments"
        }
    }
}
